    <script src="~/js/DataTables/jquery.dataTables.js"></script>
    <script src="~/js/DataTables/dataTables.bootstrap.js"></script>



    <script src="~/js/Template/js/jquery.easing.min.js"></script>

    <script src="~/js/Template/js/jqBootstrapValidation.js"></script>
    <script src="~/js/Template/js/contact_me.js"></script>
    <script src="~/js/Template/js/agency.js"></script>
    <script src="~/js/index.js"></script>

